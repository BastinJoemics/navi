{
  "version": 2,
  "name": "mytracker",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/spa"
      }
    }
  ],
  "routes": [
    {
      "src": "/data-and-history/(.*)",
      "dest": "/dist/spa/$1"
    },
    {
      "src": "/data-and-history/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/spa/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/spa/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
