{
    "version": 2,
    "name": "mytracker",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/spa"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.template.html"
      }
    ]
  }
  